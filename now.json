
{
    "name": "nextjs",
    "version": 2,
    "alias": "hanamijung.now.sh",
    "routes": [
        { "src": "/(.*)", "dest": "/build/$1"}
    ],
    "builds": [
        {"src": "build/*.*", "use": "@now/static"},
        {"src": "build/**/*.*", "use": "@now/static"}
    ],
    "public": false,
    "github": {
        "enabled": false
    }
}
